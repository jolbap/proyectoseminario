<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:clickable="true"
        android:onClick="verCasasPorMapa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mapa" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="50dp"
        android:clickable="true"
        android:onClick="verCasas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/casita" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="80dp"
        android:clickable="true"
        android:onClick="verCasasPorPrecio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton"
        app:srcCompat="@drawable/dinero" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton4"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="80dp"
        android:clickable="true"
        android:onClick="verCasasPorVecindario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton2"
        app:srcCompat="@drawable/vecindario" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="139dp"
        android:layout_height="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:text="VER CASAS POR GOOGLE MAPS"
        android:textAlignment="center"
        android:textColor="#5cf1ff"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginEnd="70dp"
        android:layout_marginTop="10dp"
        android:textAlignment="center"
        android:text="VER CASAS "
        android:textColor="#5cf1ff"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton2" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="82dp"
        android:layout_height="35dp"
        android:layout_marginBottom="100dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="8dp"
        android:text="VER CASAS POR PRECIO"
        android:textAlignment="center"
        android:textColor="#5cf1ff"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton3"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="119dp"
        android:layout_height="36dp"
        android:layout_marginBottom="95dp"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="8dp"
        android:text="VER CASAS POR VECINDARIO"
        android:textAlignment="center"
        android:textColor="#5cf1ff"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton4"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>